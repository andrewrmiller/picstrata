{
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": true,
    "quotemark": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false
  }
}